{
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "cleanCache": "rm -rf .next",
    "build": "npm run cleanCache && next build",
    "deploy": "npm run build && ./scripts/copy-to-S3.sh",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "pre-commit": "lint-staged && npm run typecheck"
  },
  "dependencies": {
    "next": "^15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.14"
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,mjs,json}": "prettier --list-different --write"
  }
}
